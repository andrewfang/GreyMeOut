<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/image_select_margin"
    android:paddingRight="@dimen/image_select_margin"
    android:paddingTop="@dimen/image_select_margin"
    android:paddingBottom="@dimen/image_select_margin"
    tools:context="edu.berkeley.cs160.andrewfang.greymeout.ImageSelectActivity.ImageSelectFragment"
    android:weightSum="1">

    <ListView
        android:id="@+id/picture_list"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:dividerHeight="0dp" />
</LinearLayout>
